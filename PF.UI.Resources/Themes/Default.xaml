                      </pf:ToggleBlock.CheckedContent>
                      <pf:ToggleBlock.UnCheckedContent>
                        <pf:Empty Background="{DynamicResource RegionBrush}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
