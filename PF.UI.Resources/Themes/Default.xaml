              <pf:ClockRadioButton x:Name="PART_ButtonPm" Grid.Row="1" Grid.Column="2" Width="30" Height="30" Margin="0,0,16,16" HorizontalAlignment="Right" VerticalAlignment="Bottom" Background="{DynamicResource PrimaryBrush}" Content="PM" Style="{StaticResource ClockRadioButtonStyle}" />
