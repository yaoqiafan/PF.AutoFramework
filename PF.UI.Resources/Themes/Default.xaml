                      </ScrollViewer>
                    </DockPanel>
                    <pf:ToggleBlock Grid.Row="1" Grid.RowSpan="2" Grid.ColumnSpan="2" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" IsChecked="{Binding HasItems, RelativeSource={RelativeSource AncestorType=ListView}, Mode=OneWay}">
