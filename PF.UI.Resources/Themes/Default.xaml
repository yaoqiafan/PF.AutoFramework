              </Grid>
              <Border Name="PART_MainBorder" Grid.Row="1" BorderBrush="{DynamicResource TitleBrush}">
                <StackPanel>
                  <TextBlock Name="MyTextBlock" MinHeight="110" MaxWidth="300" Padding="32" Background="{TemplateBinding Background}" Foreground="{DynamicResource PrimaryTextBrush}" Text="{TemplateBinding ContentStr}" TextAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" />
                  <StackPanel Margin="0,0,0,32" HorizontalAlignment="Center" Background="{DynamicResource RegionBrush}" Orientation="Horizontal">
                    <Button MinWidth="80" Margin="0,0,16,0" Command="pfcontrol:ControlCommands.Cancel" Content="取消" IsCancel="True" Visibility="{TemplateBinding ShowCancel,                                                                          Converter={StaticResource Boolean2VisibilityConverter}}" />
                    <Button MinWidth="80" Command="pfcontrol:ControlCommands.Confirm" Content="确认" IsDefault="True" Style="{StaticResource ButtonPrimary}" />
